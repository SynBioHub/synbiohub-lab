{
    "compilerOptions": {
	"outDir": "./dist/",
        "target": "es2017",
        "sourceMap": true,
        "inlineSources": false,
        "module": "commonjs",
        "baseUrl": ".",
        "alwaysStrict": false,
        "strictNullChecks": false,
        "allowJs": true,
        "paths": {
            "biocad/*": [ "node_modules/@biocad/biocad/dist/biocad/*" ],
            "jfw/*": [ "node_modules/@biocad/jfw/dist/jfw/*" ],
            "data/*": [ "node_modules/@biocad/biocad/dist/data/*" ],
            "visbolite/*": [ "node_modules/@biocad/biocad/dist/visbolite/*" ],
            "visbolite": [ "node_modules/@biocad/biocad/dist/visbolite" ]
        }
    },
    "exclude": [
        "node_modules",
        "public",
        "dist",
        "browser",
        "synbiohub.js",
        "webpack.config.js",
        "migrations"
    ]
}
