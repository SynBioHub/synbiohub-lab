
extends ../layouts/default.jade

include ../mixins/search-result.jade

block content

    div.clearfix

    div.container
        div.row
            div.col-md-6.submission-list
                h4 Private Submissions

                p You currently have #{privateSubmissions.length} private submission(s)

                each submission in privateSubmissions
                    +search-result-collection(submission,removePublicEnabled)

            div.col-md-6.submission-list
                h4 Public Submissions

                p You currently have #{publicSubmissions.length} public submission(s)

                each submission in publicSubmissions
                    +search-result-collection(submission,removePublicEnabled)



